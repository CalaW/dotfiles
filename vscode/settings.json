{
    "[cpp]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd",
        "editor.suggest.insertMode": "replace",
        "editor.wordBasedSuggestions": "matchingDocuments"
    },
    "[csharp]": {
        "editor.defaultFormatter": "csharpier.csharpier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.codeActionsOnSave": {
            "source.sort.json": "explicit"
        },
        "editor.formatOnSave": true
    },
    "[latex]": {
        "editor.formatOnPaste": false,
        "editor.indentSize": 2,
        "editor.occurrencesHighlight": "off",
        "editor.suggestSelection": "recentlyUsedByPrefix",
        "editor.tabSize": 2,
        "editor.wordWrap": "on"
    },
    "[liquid]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[plaintext]": {
        "editor.wordWrap": "on"
    },
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.fixAll": "explicit",
            "source.organizeImports": "explicit"
        },
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnSave": true
    },
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "breadcrumbs.enabled": false,
    "breadcrumbs.icons": false,
    "C_Cpp.intelliSenseEngine": "disabled",
    "clangd.arguments": [
        "-j=12",
        "--clang-tidy",
        "--background-index",
        "--all-scopes-completion",
        "--log=verbose",
        "--pretty",
        "--header-insertion=never",
        "--header-insertion-decorators",
        "--completion-style=detailed",
        "--pch-storage=memory",
        "--completion-style=detailed",
        "--fallback-style=Webkit",
        "--function-arg-placeholders=true",
        "--clang-tidy-checks=performance-*,bugprone-*,protability-*,modernize-*"
    ],
    "cmake.showOptionsMovedNotification": false,
    "cSpell.userWords": [
        "Eigen",
        "matplotlib",
        "neovim",
        "pyplot",
        "scipy",
        "sklearn",
        "Tsinghua"
    ],
    "custom-ui-style.electron": {
        "frame": false
    },
    "debug.console.fontSize": 16,
    "diffEditor.codeLens": true,
    "diffEditor.diffAlgorithm": "advanced",
    "diffEditor.hideUnchangedRegions.enabled": true,
    "diffEditor.ignoreTrimWhitespace": true,
    "doxdocgen.file.copyrightTag": [],
    "doxdocgen.generic.authorEmail": "chen-che20@mails.tsinghua.edu.cn",
    "doxdocgen.generic.authorName": "CalaW",
    "doxdocgen.generic.useGitUserEmail": true,
    "doxdocgen.generic.useGitUserName": true,
    "editor.accessibilitySupport": "off",
    "editor.cursorBlinking": "solid",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.fontFamily": "MonoLisa Variable, 苹方, SFMono Nerd Font Mono",
    "editor.fontLigatures": "'calt' on, 'liga' off, 'zero' on, 'ss04' on, 'ss12' on",
    "editor.fontSize": 16,
    "editor.fontVariations": true,
    "editor.guides.highlightActiveIndentation": "always",
    "editor.inlayHints.enabled": "on",
    "editor.inlineSuggest.enabled": true,
    "editor.lineHeight": 0,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.occurrencesHighlight": "off",
    "editor.renderWhitespace": "trailing",
    "editor.semanticHighlighting.enabled": true,
    "editor.showFoldingControls": "never",
    "editor.unicodeHighlight.allowedCharacters": {
        "α": true,
        "σ": true
    },
    "editor.unicodeHighlight.allowedLocales": {
        "zh-hans": true,
        "zh-hant": true
    },
    "editor.unicodeHighlight.invisibleCharacters": false,
    "explorer.compactFolders": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1,
        "GitHub.copilot": 2,
        "GitHub.copilot-labs": 2,
        "vscodevim.vim": 1
    },
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 10000,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.openRepositoryInParentFolders": "never",
    "github.copilot.editor.enableAutoCompletions": true,
    "github.copilot.enable": {
        "*": true,
        "markdown": true,
        "plaintext": true,
        "scminput": false,
        "yaml": false
    },
    "github.gitProtocol": "ssh",
    "gitlens.launchpad.indicator.enabled": false,
    "gitlens.launchpad.indicator.polling.enabled": false,
    "julia.enableCrashReporter": false,
    "julia.enableTelemetry": false,
    "julia.symbolCacheDownload": true,
    "latex-workshop.bibtex-fields.sort.enabled": true,
    "latex-workshop.bibtex-format.sort.enabled": true,
    "latex-workshop.bibtex-format.sortby": [
        "year-desc"
    ],
    "latex-workshop.bibtex-format.trailingComma": true,
    "latex-workshop.formatting.latex": "latexindent",
    "latex-workshop.formatting.latexindent.args": [
        "-c",
        "%DIR%/",
        "%TMPFILE%",
        "-l=%DIR%/latexindent.yaml",
        "-m",
        "-y=defaultIndent: '%INDENT%',indentRules: item: '%INDENT%'",
        "--GCString"
    ],
    "latex-workshop.hover.preview.newcommand.newcommandFile": "/Users/calaw/Documents/Program/LaTeX-Template/CalaWMath.sty",
    "latex-workshop.intellisense.citation.backend": "biblatex",
    "latex-workshop.intellisense.package.extra": [
        "minted",
        "unicode-math",
        "amsmath",
        "amssymb",
        "amsthm",
        "mathtools",
        "siunitx",
        "booktabs",
        "xcolor",
        "hyperref",
        "biblatex",
        "CalaWMath"
    ],
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.autoClean.run": "onFailed",
    "latex-workshop.latex.clean.fileTypes": [
        "*.aux",
        "*.bbl",
        "*.blg",
        "*.idx",
        "*.ind",
        "*.lof",
        "*.lot",
        "*.out",
        "*.toc",
        "*.acn",
        "*.acr",
        "*.alg",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.fls",
        "*.log",
        "*.fdb_latexmk",
        "*.snm",
        "*.synctex(busy)",
        "*.synctex.gz(busy)",
        "*.nav",
        "*.vrb",
        "*.xdv",
        "*.synctex.gz",
        "*.bcf",
        "*.run.xml",
        "_minted-*/**",
        "_minted-*/",
        "*.pyg"
    ],
    "latex-workshop.latex.clean.method": "glob",
    "latex-workshop.latex.recipe.default": "lastUsed",
    "latex-workshop.latex.recipes": [
        {
            "name": "XeLaTeX",
            "tools": [
                "latexmk-XeLaTeX"
            ]
        },
        {
            "name": "luaLaTeX",
            "tools": [
                "latexmk-luaLaTeX"
            ]
        },
        {
            "name": "pdfLaTeX",
            "tools": [
                "latexmk-pdfLaTeX"
            ]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "args": [
                "-xelatex",
                "-bibtex",
                "-synctex=1",
                "-shell-escape",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-outdir=%OUTDIR%",
                "%DOCFILE%"
            ],
            "command": "latexmk",
            "env": {},
            "name": "latexmk-XeLaTeX"
        },
        {
            "args": [
                "-lualatex",
                "-bibtex",
                "-synctex=1",
                "-shell-escape",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOCFILE%"
            ],
            "command": "latexmk",
            "env": {},
            "name": "latexmk-luaLaTeX"
        },
        {
            "args": [
                "-pdf",
                "-bibtex",
                "-synctex=1",
                "-shell-escape",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOCFILE%"
            ],
            "command": "latexmk",
            "env": {},
            "name": "latexmk-pdfLaTeX"
        }
    ],
    "latex-workshop.linting.lacheck.enabled": true,
    "latex-workshop.texcount.autorun": "onSave",
    "latex-workshop.view.pdf.external.synctex.args": [
        "-r",
        "%LINE%",
        "%PDF%",
        "%TEX%"
    ],
    "latex-workshop.view.pdf.external.synctex.command": "/Applications/Skim.app/Contents/SharedSupport/displayline",
    "latex-workshop.view.pdf.external.viewer.args": [
        "0",
        "%PDF%"
    ],
    "latex-workshop.view.pdf.external.viewer.command": "/Applications/Skim.app/Contents/SharedSupport/displayline",
    "latex-workshop.view.pdf.viewer": "external",
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.saturation": 0.6,
    "mypy-type-checker.args": [
        "--follow-imports=skip",
        "--ignore-missing-imports"
    ],
    "notebook.lineNumbers": "on",
    "python.analysis.autoFormatStrings": true,
    "python.analysis.autoImportCompletions": true,
    "python.analysis.inlayHints.functionReturnTypes": true,
    "redhat.telemetry.enabled": false,
    "remote.SSH.remotePlatform": {
        "qh.bft-robot.com": "linux"
    },
    "ruff.path": [
        "/opt/homebrew/bin/ruff"
    ],
    "scm.alwaysShowActions": true,
    "scm.alwaysShowRepositories": true,
    "security.promptForLocalFileProtocolHandling": false,
    "security.workspace.trust.untrustedFiles": "open",
    "settingsSync.ignoredExtensions": [
        "asvetliakov.vscode-neovim",
        "ms-ceintl.vscode-language-pack-zh-hans",
        "ms-vscode.cpptools",
        "ms-iot.vscode-ros",
        "vscodevim.vim",
        "sourcery.sourcery",
        "ms-dotnettools.csharp",
        "ms-dotnettools.csdevkit",
        "ms-dotnettools.vscode-dotnet-runtime",
        "ms-dotnettools.vscodeintellicode-csharp",
        "visualstudiotoolsforunity.vstuc",
        "csharpier.csharpier-vscode",
        "thqby.vscode-autohotkey2-lsp",
        "julialang.language-julia",
        "yy0931.mplstyle",
        "ms-toolsai.datawrangler"
    ],
    "settingsSync.ignoredSettings": [
        "vim.autoSwitchInputMethod.defaultIM",
        "vim.autoSwitchInputMethod.obtainIMCmd",
        "vim.autoSwitchInputMethod.switchIMCmd",
        "vim.autoSwitchInputMethod.enable",
        "vim.useCtrlKeys",
        "vim.sneak",
        "http.proxy",
        "ruff.path",
        "workbench.activityBar.location"
    ],
    "telemetry.telemetryLevel": "error",
    "terminal.external.osxExec": "Alacritty.app",
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.defaultProfile.linux": "fish",
    "terminal.integrated.defaultProfile.osx": "fish",
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "terminal.integrated.fontFamily": "SFMono Nerd Font Mono",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.gpuAcceleration": "on",
    "terminal.integrated.showExitAlert": false,
    "window.customTitleBarVisibility": "never",
    "window.titleBarStyle": "native",
    "workbench.activityBar.location": "hidden",
    "workbench.colorTheme": "Dracula Pro",
    // "workbench.colorCustomizations": {
    //     "activityBar.activeBorder": "#00000000",
    //     "activityBar.background": "#0B0A10",
    //     "scrollbar.shadow": "#00000000",
    //     "sideBar.border": "#0B0A10",
    //     "statusBarItem.remoteBackground": "#7970A9"
    // },
    "workbench.editor.showTabs": "single",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.iconTheme": "material-icon-theme",
    "workbench.layoutControl.enabled": false,
    "workbench.productIconTheme": "material-product-icons",
    "workbench.tips.enabled": false,
    "xml.format.maxLineWidth": 120,
    "xml.server.preferBinary": true,
    "zenMode.hideLineNumbers": false,
    "editor.glyphMargin": false
}