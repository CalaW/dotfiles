// Place your key bindings in this file to override the defaults
[
    {
        "command": "latex-workshop.close-env",
        "key": "cmd+] cmd+]",
        "when": "neovim.mode == insert && editorTextFocus && !virtualWorkspace && editorLangId == 'latex'"
    },
    {
        "command": "workbench.view.scm",
        "key": "shift+cmd+g",
        "when": "workbench.scm.active"
    },
    {
        "command": "-workbench.view.scm",
        "key": "ctrl+shift+g",
        "when": "workbench.scm.active"
    },
    {
        "command": "-workbench.action.terminal.findPrevious",
        "key": "shift+cmd+g",
        "when": "terminalFindFocused && terminalHasBeenCreated || terminalFindFocused && terminalProcessSupported || terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
    },
    {
        "command": "-editor.action.previousMatchFindAction",
        "key": "shift+cmd+g",
        "when": "editorFocus"
    },
    {
        "command": "workbench.view.extension.latex-workshop-activitybar",
        "key": "shift+cmd+l shift+cmd+l",
        "when": "!config.latex-workshop.bind.altKeymap.enabled"
    },
    {
        "command": "-workbench.view.extension.latex-workshop-activitybar",
        "key": "alt+cmd+x",
        "when": "!config.latex-workshop.bind.altKeymap.enabled"
    },
]